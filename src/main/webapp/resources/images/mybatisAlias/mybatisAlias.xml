<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- 
			[myBatis] 스프링에서 "_"를 사용한 컬럼명을 사용할 때 mapUnderscoreToCamelCase를
			통해 테이블 컬럼명에 "_"가 있을 경우 카멜케이스로 읽어줌
			ex) 테이블 컬럼명이 member_id인 경우 jsp 뷰단에서 이 값을 사용할 때 memberId로 사용
			단! VO만 가능! Map은 불가능!
	 -->
	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>
	
	<!-- 자주 사용하는 타입의 별칭 -->
	<typeAliases>
		<typeAlias type="com.kjh.author.vo.AuthorEmpVO" alias="authorEmpVO"/>
		<typeAlias type="com.kjh.author.vo.AuthorVO" alias="authorVO"/>

		<typeAlias type="com.kjh.bbs.vo.AnswerVO" alias="answerVO"/>
		<typeAlias type="com.kjh.bbs.vo.BbsVO" alias="bbsVO"/>

		<typeAlias type="com.kjh.common.vo.AtchmnflVO" alias="atchmnflVO"/>
		<typeAlias type="com.kjh.common.vo.CmmncodeVO" alias="cmmncodeVO"/>

		<typeAlias type="com.kjh.dclz.vo.BsrpVO" alias="bsrpVO"/>
		<typeAlias type="com.kjh.dclz.vo.CrtfreqstVO" alias="crtfreqstVO"/>
		<typeAlias type="com.kjh.dclz.vo.DclzVO" alias="dclzVO"/>
		<typeAlias type="com.kjh.dclz.vo.RestdeVO" alias="restdeVO"/>
		<typeAlias type="com.kjh.dclz.vo.VcatnVO" alias="vcatnVO"/>

		<typeAlias type="com.kjh.hr.vo.DeptVO" alias="deptVO"/>
		<typeAlias type="com.kjh.hr.vo.EmpVO" alias="empVO"/>
		
		<typeAlias type="com.kjh.mtgr.vo.MtgrResveVO" alias="mtgrResveVO"/>
		<typeAlias type="com.kjh.mtgr.vo.MtgrVO" alias="mtgrVO"/>

		<typeAlias type="com.kjh.sanction.vo.CcVO" alias="ccVO"/>
		<typeAlias type="com.kjh.sanction.vo.DrftFormVO" alias="drftFormVO"/>
		<typeAlias type="com.kjh.sanction.vo.DrftSeVO" alias="drftSeVO"/>
		<typeAlias type="com.kjh.sanction.vo.ElctrnsanctnVO" alias="elctrnsanctnVO"/>
		<typeAlias type="com.kjh.sanction.vo.SanctnLineVO" alias="sanctnLineVO"/>
		<typeAlias type="com.kjh.sanction.vo.SanctnTmprVO" alias="sanctnTmprVO"/>
		
		<typeAlias type="com.kjh.schdul.vo.SchdulVO" alias="schdulVO"/>
	</typeAliases>
</configuration>